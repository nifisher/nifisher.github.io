---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

        <title>Digital Portfolio - NIFisher</title>
        <link href="http://nifisher.github.io/blog/atom.xml" rel="self"/>
        <link href="http://nifisher.github.io/blog"/>
        <updated>{{ site.time | date_to_xmlschema }}</updated>
        <id>http://hankquinlan.github.io/blog</id>
        <author>
                <name>NIFisher</name>
                <email>fisherni@gmail.com</email>
        </author>

        &lbrace;&percnt; for post in site.posts &percnt;&rbrace;
                <entry>
                        <title>{{ post.title }}</title>
                        <link href="http://nifisher.github.io{{ post.url }}"/>
                        <updated>{{ post.date | date_to_xmlschema }}</updated>
                        <id>http://nifisher.github.io{{ post.id }}</id>
                        <content type="html">{{ post.content | xml_escape }}</content>
                </entry>
        &lbrace;&percnt; endfor &percnt;&rbrace;

</feed>
